{
  "name": "ember-playground",
  "version": "0.0.0",
  "private": true,
  "description": "Small description for ember-playground goes here",
  "repository": "",
  "license": "MIT",
  "author": "",
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "scripts": {
    "build": "ember build --environment=production",
    "lint": "npm-run-all --aggregate-output --continue-on-error --parallel 'lint:!(fix)'",
    "lint:fix": "npm-run-all --aggregate-output --continue-on-error --parallel lint:*:fix",
    "lint:hbs": "ember-template-lint .",
    "lint:hbs:fix": "ember-template-lint . --fix",
    "lint:js": "eslint {app,config,tests}/**/*.js --cache",
    "lint:js:fix": "eslint {app,config,tests}/**/*.js --fix",
    "lint:css": "stylelint **/*.css,scss --cache",
    "lint:css:fix": "stylelint **/*.css,scss --fix",
    "start": "ember serve",
    "test": "npm-run-all lint test:*",
    "test:ember": "ember test",
    "storybook": "start-storybook -p 6006 -s dist",
    "build-storybook": "ember build --environment test && build-storybook -s dist",
    "clean": "rimraf dist storybook-static"
  },
  "devDependencies": {
    "@babel/core": "^7.13.10",
    "@ember/optional-features": "^2.0.0",
    "@ember/test-helpers": "^2.2.0",
    "@glimmer/component": "^1.0.3",
    "@glimmer/tracking": "^1.0.3",
    "@percy/ember": "^2.1.4",
    "@storybook/addon-a11y": "^6.1.21",
    "@storybook/addon-backgrounds": "^6.1.21",
    "@storybook/addon-controls": "^6.1.21",
    "@storybook/addon-docs": "^6.1.21",
    "@storybook/addon-toolbars": "^6.1.21",
    "@storybook/addon-viewport": "^6.1.21",
    "@storybook/ember": "^6.1.21",
    "@storybook/ember-cli-storybook": "^0.3.1",
    "@testing-library/dom": "^7.30.1",
    "@testing-library/user-event": "^13.0.15",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.2.2",
    "babel-plugin-ember-modules-api-polyfill": "^3.5.0",
    "babel-plugin-htmlbars-inline-precompile": "^5.1.0",
    "broccoli-asset-rev": "^3.0.0",
    "ember-a11y-testing": "^4.0.2",
    "ember-auto-import": "^1.10.1",
    "ember-cli": "~3.25.2",
    "ember-cli-app-version": "^4.0.0",
    "ember-cli-babel": "^7.23.1",
    "ember-cli-dependency-checker": "^3.2.0",
    "ember-cli-htmlbars": "^5.3.2",
    "ember-cli-inject-live-reload": "^2.0.2",
    "ember-cli-sri": "^2.1.1",
    "ember-cli-storybook": "^0.1.0",
    "ember-cli-terser": "^4.0.1",
    "ember-css-modules": "^1.3.4",
    "ember-css-modules-reporter": "^1.2.0",
    "ember-css-modules-stylelint": "^1.2.0",
    "ember-data": "~3.25.0",
    "ember-export-application-global": "^2.0.1",
    "ember-fetch": "^8.0.4",
    "ember-load-initializers": "^2.1.2",
    "ember-maybe-import-regenerator": "^0.1.6",
    "ember-page-title": "^6.2.1",
    "ember-qunit": "^5.1.2",
    "ember-resolver": "^8.0.2",
    "ember-sinon-qunit": "^5.0.0",
    "ember-source": "~3.25.1",
    "ember-template-imports": "^1.1.1",
    "ember-template-lint": "^2.18.1",
    "eslint": "^7.20.0",
    "eslint-config-prettier": "^7.2.0",
    "eslint-plugin-ember": "^10.2.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.3.1",
    "husky": "^5.2.0",
    "lint-staged": "^10.5.4",
    "loader.js": "^4.7.0",
    "msw": "^0.28.0",
    "npm-run-all": "^4.1.5",
    "postcss-preset-env": "^6.7.0",
    "prettier": "^2.2.1",
    "qunit": "^2.14.0",
    "qunit-dom": "^1.6.0",
    "rimraf": "^3.0.2",
    "yarn": "^1.22.10"
  },
  "engines": {
    "node": "10.* || >= 12"
  },
  "ember": {
    "edition": "octane"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "{app,config,tests}/**/*.js": [
      "eslint --fix"
    ],
    "{app,config,tests}/**/*.hbs": [
      "ember-template-lint --fix"
    ],
    "{app,config,tests}/**/*.css": [
      "stylelint --allow-empty-input --fix"
    ]
  },
  "msw": {
    "workerDirectory": "public"
  }
}
